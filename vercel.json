{
  "rewrites": [
    {
      "source": "/",
      "has": [
        { "type": "host", "value": "koomy.app" }
      ],
      "destination": "/website"
    },
    {
      "source": "/website/:path*",
      "has": [
        { "type": "host", "value": "koomy.app" }
      ],
      "destination": "/website/:path*"
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "koomy.app" }
      ],
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "koomy-saas-git-main-rites-projects-0af03fac.vercel.app"
        }
      ],
      "destination": "https://koomy.app/$1",
      "permanent": true
    }
  ]
}
