{
  "rewrites": [
    {
      "source": "/",
      "has": [
        { "type": "host", "value": "koomy.app" }
      ],
      "destination": "/website"
    },
    {
      "source": "/:path*",
      "has": [
        { "type": "host", "value": "koomy.app" }
      ],
      "destination": "/website/:path*"
    }
  ]
}
